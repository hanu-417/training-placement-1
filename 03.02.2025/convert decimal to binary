def decimal_to_binary(decimal):
    binary = ""
    quotient = decimal
    while quotient > 0:
        remainder = quotient % 2
        binary = str(remainder) + binary
        quotient //= 2
    return binary
decimal_number = 10
binary_number = decimal_to_binary(decimal_number)
print("Binary representation of", decimal_number, "is", binary_number)
